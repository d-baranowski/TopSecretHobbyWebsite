@model IEnumerable<MtgCollectionWebApp.Models.CollectionEntry>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Card Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CollectionId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            Card Name
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.CollectionId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CollectionEntryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CollectionEntryId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CollectionEntryId })
        </td>
    </tr>
}

</table>
